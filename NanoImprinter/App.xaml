<prism:PrismApplication x:Class="NanoImprinter.App"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:local="clr-namespace:NanoImprinter"
                        xmlns:prism="http://prismlibrary.com/">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/UITheme;component/UIModuleResource.xaml" />
                <ResourceDictionary Source="Styles/BaseControlStyle.xaml" />
                <ResourceDictionary Source="Styles/ComboBoxStyle.xaml" />
                <ResourceDictionary Source="Styles/GroupBoxStyle.xaml" />
                <ResourceDictionary Source="Styles/IconButtonStyle.xaml" />
                <ResourceDictionary Source="Styles/IconTabItemStyle.xaml" />
                <ResourceDictionary Source="Styles/ParameterDisplayControlStyle.xaml" />
                <ResourceDictionary Source="Styles/SwitchButtonStyle.xaml" />
                <ResourceDictionary Source="Styles/TabControlStyle.xaml" />
                <ResourceDictionary Source="Styles/OtherControlsStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</prism:PrismApplication>
